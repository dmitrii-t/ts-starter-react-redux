{
  "name": "ts-starter-react-redux",
  "version": "0.0.1",
  "description": "ts-starter-react-redux",
  "main": "dist/main-bundle.js",
  "license": "MIT",
  "engines": {
    "node": ">=10.x"
  },
  "scripts": {
    "start:hot": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack-dev-server -w --hot --open --content-base dist/",
    "start": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack-dev-server -w --open --content-base dist/",
    "watch": "cross-env TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack --watch --progress",
    "build": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack -p",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx'",
    "clean": "rimraf dist",
    "test": "jest"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 120
  },
  "keywords": [
    "typescript",
    "react",
    "starter-kit",
    "webpack",
    "node"
  ],
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupEnzyme.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/src/tsconfig-for-tests.json"
      }
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "axios": "^0.19.0",
    "body-parser": "^1.18.3",
    "dotenv": "^8.0.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "find-up": "^4.0.0",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@material-ui/core": "^3.8.3",
    "@types/cssnano": "^4.0.0",
    "@types/enzyme": "^3.1.15",
    "@types/express": "^4.16.0",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/jest": "^24.0.13",
    "@types/node": "^10.14.10",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^4.3.5",
    "@types/webpack": "^4.4.34",
    "@types/webpack-bundle-analyzer": "^2.13.1",
    "@types/webpack-dev-server": "^3.1.6",
    "@types/webpack-manifest-plugin": "^2.0.0",
    "babel-loader": "^8.0.5",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.10",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.19.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "postcss-loader": "^3.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "request": "^2.88.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tsc-watch": "^1.0.31",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.12.1",
    "typescript": "^3.5.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.32.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.4"
  }
}
